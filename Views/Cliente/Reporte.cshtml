@model IEnumerable<Proyecto.Models.Reporte>

@{
    ViewBag.Title = "Reporte";
}

<h2>Reporte</h2>

<table class="table">
    <tr>
        <th>Nombre del Cliente</th>
        <th>Email del Cliente</th>
        <th>Id Compra</th>
        <th>Total Compra</th>
    </tr>
    @foreach (var item in Model)
    {

<tr>

        <td>@Html.DisplayFor(modelItem => item.nombreCliente)</td>
        <td>@Html.DisplayFor(modelItem => item.emailCliente)</td>
        <td>@Html.DisplayFor(modelItem => item.idCompra)</td>
        <td>@Html.DisplayFor(modelItem => item.totalCompra)</td>

</tr>

    }
</table>

